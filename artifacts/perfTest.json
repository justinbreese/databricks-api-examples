    "FileSize": "uipConfig",
    "ReadQuery": "auth_id = 1064410930",
    "JobConfig": {
        "run_name": "${runName}",
        "notebook_task": {
            "notebook_path": "/Users/Justin_AllanBreese@intuit.com/live_tables/mor/main",
            "base_parameters":
                {
                    "operation": "${operation}",
                    "method": "${method}",
